dct={1:"naruto",2:"goku",3:"luffy"}
print(dct)
dct_views=dct.items()    
print(dct_views)     
print(type(dct_views))
g=dct.keys()
f=dct.values()
print(f)
print(g)
dct_list_keys=list(g)
print(dct_list_keys)
dct_list_values=list(f)
print(dct_list_values)
zipp=zip(g,f)
print(zipp)
zipplist=list(zipp)
print(type(zipplist))
print(zipplist)
dc_from_zipp=dict(zipplist)
